{"components/common/quick-search":"<div id=\"search\" class=\"desktop-search d-block sideb siden\">\n{{{ snippet \"forms_search\"}}}\n<div id=\"search\" class=\"wbSearch\">\n     <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"wb-search input-group\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input form-control\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <div class=\"input-group-btn\">\n                    <button class=\"wb-search-btn\" align=\"center\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\" aria-label=\"search\">\n                        <svg width=\"20px\" height=\"20px\">\n                            <use xlink:href=\"#mysearch\"></use>\n                        </svg>\n                          \n                          <!-- {{lang \"search.quick_search.input_label\"}} -->\n                    </button>\n                  </div>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n\n</div>\n</div>"}
