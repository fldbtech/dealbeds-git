{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<li class=\"mycurry acd\">\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item  mycurrency\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n</li>\n{{/if}}\n","components/common/navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        <!-- account accsess for user -->\n        <li class=\"navUser-item head-user user-info myaccount\">\n            <a class=\"navUser-action\"  data-dropdown=\"account-dropdown\" aria-label=\"user_account\" href=\"#\">\n              <span class=\"svgbg navUser-item-cartLabel list-inline\">\n                {{#if theme_settings.theme_multistore}} \n                <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#huser\"></use></svg>\n                <!-- <h5 class=\"text-left\">{{lang 'login / register'}}</h5> -->\n                {{else}}\n                    {{#if theme_settings.theme_fashion}}\n                    <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#huser-fashion\"></use></svg>\n                <!-- <h5 class=\"text-left\">{{lang 'login / register'}}</h5> -->\n                {{else}}\n                {{#if theme_settings.theme_kids}}\n                    <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#huser-fashion\"></use></svg>\n                {{/if}}\n                    {{/if}}\n                {{/if}}\n              </span>\n          </a>\n          <ul class=\"dropdown-menu user-down dropdown-menu-left headlog\" id=\"account-dropdown\" data-dropdown-content aria-hidden=\"true\">\n            <h5 class=\"text-left\">{{lang 'Your Account'}}</h5>\n            <h6 class=\"text-left\">{{lang 'Access account and manage orders'}}</h6>\n            {{#if customer}}\n                <p class=\"user-name\">{{lang 'Hi! '}}{{first (split customer.name \" \") }}</p>\n            {{/if}}\n            {{#if customer.store_credit.value '>' 0}}\n                <div class=\"mystorecredit\">\n                    <ul class=\"navUser-section\">\n                        <li class=\"navUser-item\">\n                            <a class=\"navUser-action navUser-action--storeCredit has-dropdown\"\n                               data-dropdown=\"storeCredit-dropdown\"\n                               aria-controls=\"storeCredit-dropdown\"\n                               href=\"{{urls.cart}}\"\n                               aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                            >\n                                <span class=\"navUser-action-divider\">\n                                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                                </span>\n                            </a>\n                            <ul class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n                                {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                            </ul>\n                        </li>\n                    </ul>\n                </div>\n            {{/if}}\n            {{#if customer}}\n            <li class=\"acd\">\n                <a class=\"navUser-action dropdown-menu-item sign_out dro\" href=\"{{urls.auth.logout}}\"><i class=\"fa fa-sign-out\"></i>{{lang 'common.logout'}}</a>\n            </li>\n            <li class=\"acd account-dropdown\">\n                <a class=\"navUser-action dropdown-menu-item my_account dro\" href=\"{{urls.account.index}}\"><i class=\"fa fa-user\"></i>{{lang 'common.account'}}</a>\n            </li>\n            {{else}}\n            <li class=\" mylogin acd\">\n              <a class=\"navUser-action dropdown-menu-item dro\" href=\"{{urls.auth.login}}\"><i class=\"fa fa-lock\"></i>{{lang 'common.login'}}</a>\n            </li>\n            {{#if settings.account_creation_enabled}}\n            <li class=\" mycreat acd\">\n              <a class=\"navUser-action dropdown-menu-item dro\" href=\"{{urls.auth.create_account}}\">\n                <i class=\"fa fa-user-plus\"></i>{{lang 'common.sign_up'}}\n              </a>\n            </li>\n            {{/if}}\n            {{/if}}\n            <li class=\" acd\">\n                    <a class=\"navUser-action navUser-item--compare\"\n                       href=\"{{urls.compare}}\"\n                       data-compare-nav\n                       aria-label=\"{{lang 'common.compare'}}\"\n                    ><i class=\"fa fa-compress\"></i>\n                        {{lang 'common.compare'}} <!-- <span class=\"countPill countPill--positive countPill--alt\"></span> -->\n                    </a>\n            </li>\n            \n            {{> components/common/currency-selector}}\n            \n                \n          </ul>\n        </li>\n\n        <!-- Wishlist Icon -->\n        <li class=\"mywishcus\">\n            <div class=\"navUser-item navUser-item--wishlist\">\n                    <a class=\"cart-button cart-button--wishlist dropdown-menu-item navUser-action\" title=\"{{lang 'account.nav.wishlists'}}\" href=\"{{urls.account.wishlists.all}}\">\n                        <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#hwishlist\"></use></svg>\n                        <span>{{lang \"common.wishlist\"}}</span>\n                    </a>\n            </div>\n        </li>\n        <!-- cart icon  -->\n        <li class=\"navUser-item navUser-item--cart header-cart--{{theme_settings.cart-type}}\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n          \n                <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#hcart\"></use></svg>\n           \n            \n                <!-- <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> -->\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"><div class=\"abc\">close</div></div>\n        </li>\n    </ul>\n    \n</nav>\n"}
