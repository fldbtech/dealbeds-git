{"components/cms/lookbook":"<div class=\"section-spacing\">\n    <div class=\"container\">\n        <div class=\"home_most_popular_products\">\n            <div class=\"myheadeing\">\n                <h2 class=\"page-heading\">\n                {{theme_settings.lookbook-heading}}\n                <p>{{theme_settings.subheading-lookbook}}</p>\n                <span class=\"head-line\"></span>\n                </h2>\n            </div>\n            <div class=\"mp_parent_slider\" \n                data-slick='{\n                    \"arrows\": true,\n                    \"dots\": false,\n                    \"slidesToShow\": 1,\n                    \"slidesToScroll\": 1,\n                    \"prevArrow\": \".js-product-prev-arrow-lookbook\",\n                    \"nextArrow\": \".js-product-next-arrow-lookbook\"\n                }' >\n\n            \n                <!-- Slider 1 -->\n                <div class=\"mp_product_list\">\n                    <div class=\"mp_inner\">\n                        <div class=\"image_section\">\n                            <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_1_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\n                            {{#each (split theme_settings.mp_slider_1_pIdsDots \",\") as |productDots| }}\n                                {{#each (split ../theme_settings.mp_slider_1_pIds \",\") as |productIds| }}\n                                    {{#if @../index '==' @index}}\n                                        <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\n                                    {{/if}}\n                                {{/each}}\n                            {{/each}}\n                        </div>\n                        <div class=\"product_section mp-pro-data-1\"></div>\n                        <input type=\"hidden\" id=\"mp_slider_1_data\" value=\"{{theme_settings.mp_slider_1_pIds}}\" />\n                    </div>\n                </div>\n\n                <!-- Slider 2 -->\n                <div class=\"mp_product_list\">\n                    <div class=\"mp_inner\">\n                        <div class=\"image_section\">\n                            <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_2_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\n                            {{#each (split theme_settings.mp_slider_2_pIdsDots \",\") as |productDots| }}\n                                {{#each (split ../theme_settings.mp_slider_2_pIds \",\") as |productIds| }}\n                                    {{#if @../index '==' @index}}\n                                        <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\n                                    {{/if}}\n                                {{/each}}\n                            {{/each}}\n                        </div>\n                        <div class=\"product_section mp-pro-data-2\"></div>\n                        <input type=\"hidden\" id=\"mp_slider_2_data\" value=\"{{theme_settings.mp_slider_2_pIds}}\" />\n                    </div>\n                </div>\n\n                <!-- Slider 3 -->\n                <div class=\"mp_product_list\">\n                    <div class=\"mp_inner\">\n                        <div class=\"image_section\">\n                            <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_3_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\n                            {{#each (split theme_settings.mp_slider_3_pIdsDots \",\") as |productDots| }}\n                                {{#each (split ../theme_settings.mp_slider_3_pIds \",\") as |productIds| }}\n                                    {{#if @../index '==' @index}}\n                                        <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\n                                    {{/if}}\n                                {{/each}}\n                            {{/each}}\n                        </div>\n                        <div class=\"product_section mp-pro-data-3\"></div>\n                        <input type=\"hidden\" id=\"mp_slider_3_data\" value=\"{{theme_settings.mp_slider_3_pIds}}\" />\n                    </div>\n                </div>                \n            </div>\n            <div class=\"all-btn recent-post\">                \n                <button class=\"js-product-prev-arrow-lookbook slick-prev slick-arrow\"><svg width=\"38px\" height=\"26px\"><use xlink:href=\"#arrow-left\"></use></svg></button>\n                <button class=\"js-product-next-arrow-lookbook slick-next slick-arrow\"><svg width=\"38px\" height=\"26px\"><use xlink:href=\"#arrow-right\"></use></svg></button>            \n            </div>\n        </div>\n    </div>\n</div>"}
